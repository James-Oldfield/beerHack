<body ontouchmove="blockMove()">

    <div id="test"></div>

    <div class="container">
        <div class="top" id="showdiv4"><img src="banner.png"></div>
        
        <div class="mid">
        
            <div class="food" id="showdiv1"><span class="txt"><i class="fa fa-birthday-cake"></i></span></div>
